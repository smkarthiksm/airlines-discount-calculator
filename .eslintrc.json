{
    "env": {
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "classPrivateMethods",
        "babel"
      ],
    "rules": {
        "import/prefer-default-export": 0,
        "no-plusplus": 0
    }
}
